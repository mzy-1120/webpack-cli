{
  "name": "webpack-cli",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/mzy911/webpack-cli.git",
  "author": "mfr",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "env-cmd -f .env.development webpack serve --config ./config/webpack.dev.js",
    "build": "env-cmd -f .env.production webpack --config ./config/webpack.build.js",
    "build:dll": "env-cmd -f .env.production webpack --config ./config/webpack.dll.js",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/runtime-corejs3": "^7.26.0",
    "@eslint/js": "^9.17.0",
    "@types/jquery": "^3.5.32",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.1",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "babel-loader": "^9.2.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.13.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "less": "^4.2.1",
    "less-loader": "^12.2.0",
    "lint-staged": "^15.2.11",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.4.49",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.1.1",
    "prettier": "^3.4.2",
    "sass": "^1.82.0",
    "sass-loader": "^16.0.4",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "thread-loader": "^4.0.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "dependencies": {
    "jquery": "^3.7.1",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.46"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,css,less,sass,json,vue}": [
      "eslint --fix",
      "git add"
    ]
  }
}
